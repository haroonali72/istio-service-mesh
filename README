-e --PORT \
-e 8654 \
-e --REDIS_ENGINE_URL \
-e "10.248.9.173:6379" \
-e --LOGGING_ENGINE_URL \
"http://10.248.9.173:3500" --KUBERNETES_ENGINE_URL "http://10.248.9.173:8089/api/v1/kubernetes/deploy"